      "dataset_num_proc": 2,
      "packing": false,
      "optim": "adamw_8bit"
    },
    "lora_r": 16,
    "lora_target_modules": ["q_proj", "k_proj", "v_proj", "o_proj"],
    "lora_alpha": 16,
    "lora_dropout": 0.05,
    "lora_bias": "none",
    "use_gradient_checkpointing": true,
    "random_state": 42,
    "use_rslora": false,
    "loftq_config": null
