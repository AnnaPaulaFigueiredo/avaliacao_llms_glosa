{
    "wandb_api_key": "",
    "hf_read_token": "",
    "hf_write_token": "",
    "project_name": "Chatbot-Finetune-UnslothV1",
    "output_base_dir": "./checkpoints_unsloth/v1",
    "models": [
 
        {
          "model_name": "unsloth/DeepSeek-R1-Distill-Llama-8B", 
          "dataset_name": "annagoncalves2/glosa",
          "repo_name": "chatbot-unsloth-DeepSeek-R1-Distill-Llama-8B-test"
        },
        {
          "model_name": "unsloth/Qwen3-14B-unsloth-bnb-4bit",
          "dataset_name": "annagoncalves2/glosa",
          "repo_name": "chatbot-Qwen3-14B-unsloth-bnb-4bit-test"
        }
    ]
    ,
    "training_args": {
